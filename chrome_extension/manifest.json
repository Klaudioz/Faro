{
  "manifest_version": 3,
  "name": "Page Content Reader",
  "version": "1.0",
  "description": "Reads and logs webpage content to console",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
